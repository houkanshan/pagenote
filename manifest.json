{
   "name":"pagenote",
    "version": "0.1",
    "description":"just like evernote in chrome",
    "browser_action": {
      "default_title": "pagenote",
      "default_icon": "icons/icon48.png",
      "default_popup": "popup.html"
    },
    "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon48.png",
      "48": "icons/icon48.png"
    },
    "permissions": [
      "tabs",
      "notifications",
      "unlimitedStorage",
      "http://*/*", 
      "https://*/*"
    ],

    "background_page": "background.html"
}
